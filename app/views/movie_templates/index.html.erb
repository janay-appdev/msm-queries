<table border = "1">
    <tr>
     <th>ID</th>
     <th>Image</th>
     <th>Name</th>
     <th>Dob</th>
     <th></th>
   </tr>
 <% @list_of_movies.each do |a_thing|%>
   <tr>
     <td><%= a_thing.id %></td>
     <td>
       <img src="http://robohash.org/<%= a_thing.name%>">
     </td>
     <td><%= a_thing.name %></td>
     <td><%= a_thing.dob %></td>
     <td>
       <a href="/movies/<%= a_thing.id %>">
         Show details
       </a>
     </td>
   </tr>
 <% end %>
</table>
